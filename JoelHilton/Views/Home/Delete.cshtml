@{
    ViewData["Title"] = "Delete Movie Entry";
}

@model MovieResponses

<h2>Please confirm that you wish to delete this record</h2>

<form method="post" asp-action="Delete">
    <button type="submit">Delete</button>
    <a asp-controller="Home" asp-action="MovieList" class="btn btn-primary">Cancel</a>

    <input type="hidden" asp-for="MovieId"/>
</form>